+package com.pdfwizard.pro.ui.theme
+
+import androidx.compose.ui.graphics.Color
+
+val PrimaryBlue = Color(0xFF0058FB)
+val PrimaryBlueContainer = Color(0xFFDDE1FF)
+val OnPrimary = Color.White
+val Secondary = Color(0xFF5B5D72)
+val Background = Color(0xFFFDFBFF)
+val Surface = Color(0xFFFDFBFF)
+val OnSurface = Color(0xFF1A1C1E)
diff --git a/app/src/main/java/com/pdfwizard/pro/ui/theme/Theme.kt b/app/src/main/java/com/pdfwizard/pro/ui/theme/Theme.kt
new file mode 100644
index 0000000000000000000000000000000000000000..9dff1f1fd651a7451cd1f8776e059e49bae2925b
